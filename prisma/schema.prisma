model change {
  id          Int       @id @default(autoincrement())
  oldData     String
  newData     String
  tableName   String
  columnName  String
  createdAt   DateTime  @default(now())
  createdById Int
  updatedAt   DateTime  @updatedAt
  updatedById Int?
  deletedAt   DateTime?
  deletedById Int?
  createdBy   user      @relation("change_createBy", fields: [createdById], references: [id])
  deletedBy   user?     @relation("change_deletedBy", fields: [deletedById], references: [id])
  updatedBy   user?     @relation("change_updatedBy", fields: [updatedById], references: [id])

  @@index([createdById], map: "change_createdById_fkey")
  @@index([deletedById], map: "change_deletedById_fkey")
  @@index([updatedById], map: "change_updatedById_fkey")
}

